comp_12=`float noise4 (float2 uvi) { float n1,n2,n3,n4,zv; zv = 0.006*time;
comp_13=`  n1 = lum(tex2D(sampler_noise_hq, (uvi)));
comp_14=`  n2 = lum(tex2D(sampler_noise_hq, (uvi*2+zv)));
comp_15=`  n3 = lum(tex2D(sampler_noise_hq, (uvi*4+zv)));
