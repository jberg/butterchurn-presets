per_frame_3=ob_r = ob_r + 0.8*sin(1.1*time) + 1.1*cos(2*time);
per_frame_4=ob_b = ob_b + 0.9*sin(1.2*time) + cos(1.5*time);
per_frame_5=ob_g = ob_g + 0.7*sin(time) + sin(0.4*time);
per_frame_6=wave_r = abs(ob_r+0.2);
per_frame_7=wave_g = abs(ob_g+0.2);
per_frame_8=wave_b = abs(ob_b+0.2);
per_frame_9=ib_b = ib_b + 0.44*cos(1.5*time) + 0.7*tan(time);
per_frame_10=ib_g = ib_g + 2*sin(time*bass_att);
per_frame_11=ib_r = ib_r + 0.8*sin(1.2*time) + 1.3*cos(1.8*time);
per_pixel_1=thresh = above(bass_att,thresh)*2+(1-above(bass_att,thresh))*((thresh-1.3)*0.96+1.3);
per_pixel_2=dx_r = equal(thresh,2)*0.015*sin(5*time)+(1-equal(thresh,2))*dx_r;
per_pixel_3=dy_r = equal(thresh,2)*0.015*sin(6*time)+(1-equal(thresh,2))*dy_r;
per_pixel_4=zoom = zoom + 0.07*treb*(abs(sin(time*rad)))*(0.3-rad);
per_pixel_5=dy = dy + dy_r*below(rad,0.5 - (0.2*time));
per_pixel_6=dx = dx + dx_r*below(rad,0.5 - (0.2*time));
per_pixel_7=rot = rot + (0.7*bass_att*sin(abs(dx_r*(1-rad)*2*time+cos(rad*12))))*0.2;
warp_1=`shader_body
warp_2=`{
warp_3=`    // sample previous frame
warp_4=`    ret = tex2D( sampler_main, uv ).xyz;
warp_5=`
warp_6=`    // take the difference between the crisp and blurred images,
warp_7=`    // then add it back into the image.  Creates spots and stripes over time.
warp_8=`    ret += (ret - GetBlur1(uv))*0.3;
warp_9=`    ret *= 0.9;
warp_10=`
warp_11=`   // add noise:
warp_12=`   float2 dither_uv = uv_orig*texsize.xy*texsize_noise_lq.zw * 0.1;// + rand_frame.xy;
warp_13=`   ret += (tex2D(sampler_noise_lq, dither_uv).xyz-0.5)/256.0 * 28;
warp_14=`
warp_15=`   // desaturate over time, to keep the globs white
warp_16=`   ret = lerp(ret, lum(ret), 0.04);
warp_17=`}
