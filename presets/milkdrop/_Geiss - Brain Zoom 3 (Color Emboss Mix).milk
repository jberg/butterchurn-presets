warp_1=`shader_body
warp_2=`{
warp_3=`    ret = tex2D( sampler_main, uv ).xyz;
warp_4=`
warp_5=`    // ERROR DIFFUSION DITHER - looks great
warp_6=`    float2 dither_uv = uv_orig*texsize.xy*texsize_noise_lq.zw + rand_frame.xy;
warp_7=`    ret += (tex2D(sampler_noise_lq, dither_uv).xyz-0.5)/256.0*3;
warp_8=`
warp_9=`    // darken over time
warp_10=`    //ret -= 0.004;
warp_11=`    //ret *= 0.99;
warp_12=`    //ret *= 0.97 + 0.03*saturate( length(uv - uv_orig)*200 );  // center only
warp_13=` 
warp_14=`    ret = frac(ret - 0.005);
warp_15=`}
comp_1=`float2 uv_x, uv_y, uv_z, uv_echo;
comp_2=`shader_body
comp_3=`{
comp_4=`    float rad_hq = length(uv-0.5);
comp_5=`    float2 d = texsize.zw*4;
comp_6=`    float f = 0.7;
comp_7=`    float3 N;
comp_8=`    N.x = ( GetBlur2(uv + float2(1,0)*d) - GetBlur2(uv-float2(1,0)*d) ).x*f;
comp_9=`    N.y = ( GetBlur2(uv + float2(0,1)*d) - GetBlur2(uv-float2(0,1)*d) ).x*f;
comp_10=`    N.x += ( GetBlur1(uv + float2(1,0)*d) - GetBlur1(uv-float2(1,0)*d) ).x*(1-f);
comp_11=`    N.y += ( GetBlur1(uv + float2(0,1)*d) - GetBlur1(uv-float2(0,1)*d) ).x*(1-f);
comp_12=`    N.z = -0.07;
comp_13=`    N = normalize(N);
comp_14=`
comp_15=`    ret = N;
comp_16=`
comp_17=`    float3 light_pos = float3(q6,q7,-0.8);
comp_18=`
comp_19=`       
comp_20=`    float3 P = float3(uv_orig.xy*2-1, 0);
comp_21=`    float3 L = normalize(light_pos - P);
comp_22=`    float3 E = float3(0,0,1);
comp_23=`    float3 V = normalize(P-E);
comp_24=`    float3 R = normalize( V + 2*N*dot(V,N) );
comp_25=`
comp_26=`    // diffuse lighting:
comp_27=`    ret = saturate(dot(N,L));
comp_28=`    //ret *= float3(1.2,0.9,0.6);    
comp_29=`    ret *= normalize( GetBlur3(uv_orig).yzx + 0.05 );
comp_30=`    //ret = 0;
comp_31=`
comp_32=`    // spec lighting:
comp_33=`    ret += pow(saturate(dot(R,L)),32)*0.5;
comp_34=`      
comp_35=`    //ret *= 0.7;
comp_36=`}
